### 7 (7.A1) --- HRA TIPOVAČKA - špinavý kód ---

import random

print("--- HRA TIPOVAČKA ---\nUhodni číslo, které padne.\n\nMáš 5 životů,\nza každé neuhodnutí\npřicházíš o život.\n")


zivotu = 5


while True:

    prvni_uzivatelsky = int(input("1. level - Zadej číslo od 1-2: "))
    prvni_pc = random.randint(1, 2)

    if prvni_uzivatelsky == prvni_pc:
        print(f"Padlo {prvni_pc}, trefil jsi, gratuluji!\nMáš {zivotu} životy.\n")

    else:
        zivotu -= 1
        print(f"Padlo {prvni_pc}, netrefil jsi, přicházíš o život.\nMáš {zivotu} životy.\n")
        if zivotu == 0:
            break


    if prvni_uzivatelsky == prvni_pc:
        druhy_uzivatelsky = int(input("2. level - Zadej číslo od 1-3: "))
        druhy_pc = random.randint(1, 3)

